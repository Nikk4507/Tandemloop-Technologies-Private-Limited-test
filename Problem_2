import java.util.*;

class Main {
    public static void main(String[] args) {

        int x, oddNum = 1;
        Scanner sc = new Scanner(System.in);

        System.out.println("Enter the value of x:");
        x = sc.nextInt();

        System.out.print("Output:");
        for(int i=0;i<x;i++){
            System.out.print(oddNum+",");
            oddNum += 2;
        }

        System.out.println("\nName: Nikhil Verma, Thank You!!");

        sc.close();
    }
}
